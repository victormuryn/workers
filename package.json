{
  "private": true,
  "name": "workers",
  "version": "1.0.0",
  "description": "Main repository for Workers web-site. Open README.md for instructions",
  "main": "app.ts",
  "proxy": "http://localhost:5000",
  "author": "Victor Muryn",
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/config": "0.0.38",
    "@types/jsonwebtoken": "8.5.1",
    "@types/multer": "1.4.5",
    "@types/sharp": "0.27.3",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "concurrently": "5.3.0",
    "eslint": "7.19.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-react": "7.22.0",
    "nodemon": "2.0.7",
    "ts-node": "9.1.1",
    "typescript": "4.2.3"
  },
  "dependencies": {
    "@squoosh/cli": "0.6.3",
    "@types/express": "^4.17.11",
    "all-the-cities": "3.1.0",
    "bcrypt": "5.0.1",
    "config": "3.3.6",
    "cross-env": "^7.0.3",
    "csv-parse": "4.15.3",
    "express": "4.17.1",
    "express-formidable": "1.2.0",
    "express-validator": "6.9.2",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.11.15",
    "multer": "1.4.2",
    "node-geocoder": "3.27.0",
    "sharp": "0.27.2",
    "socket.io": "3.1.2"
  },
  "scripts": {
    "eslint": "eslint --ext .jsx --ext .js",
    "test": "npm run eslint",
    "server": "nodemon app.ts",
    "dev": "npx cross-env NODE_ENV=development concurrently \"npm run client\" \"npm run server\"",
    "start": "npx cross-env NODE_ENV=production npx ts-node app.ts",
    "client": "npm start --prefix client",
    "client:build": "npm run build --prefix client",
    "build": "npm run client:build",
    "install": "npm install & npm install --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victormuryn/workers.git"
  },
  "bugs": {
    "url": "https://github.com/victormuryn/workers/issues"
  },
  "homepage": "https://github.com/victormuryn/workers#readme"
