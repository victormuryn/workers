{
  "private": true,
  "name": "workers",
  "version": "1.0.0",
  "description": "Main repository for Workers web-site. Open README.md for instructions",
  "main": "app.js",
  "proxy": "http://localhost:5000",
  "author": "Victor Muryn",
  "devDependencies": {
    "concurrently": "5.3.0",
    "eslint": "7.19.0",
    "eslint-config-google": "0.14.0",
    "nodemon": "2.0.7"
  },
  "dependencies": {
    "bcrypt": "5.0.0",
    "config": "3.3.3",
    "express": "4.17.1",
    "express-validator": "6.9.2",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.11.15"
  },
  "scripts": {
    "testClient": "npm test --prefix client",
    "eslint": "eslint --ext .jsx --ext .js",
    "testServer": "npm run eslint",
    "test": "npm run testClient && npm run testServer",
    "server": "nodemon app.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "start": "node app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victormuryn/workers.git"
  },
  "bugs": {
    "url": "https://github.com/victormuryn/workers/issues"
  },
  "homepage": "https://github.com/victormuryn/workers#readme"
}
